{
  "name": "product-recommendation-service",
  "version": "1.0.0",
  "description": "Hybrid product recommendation service with MongoDB and Redis",
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js server",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts server",
    "worker": "node dist/cli.js worker",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/cli.ts worker",
    "job": "node dist/cli.js job",
    "cli": "node dist/cli.js",
    "cli:dev": "ts-node src/cli.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "seed": "ts-node scripts/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "recommendations",
    "collaborative-filtering",
    "machine-learning",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/connect-timeout": "^1.9.0",
    "bullmq": "^5.66.4",
    "commander": "^14.0.2",
    "compression": "^1.8.1",
    "connect-timeout": "^1.9.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "mathjs": "^15.1.0",
    "ml-distance": "^4.0.1",
    "mongodb": "^7.0.0",
    "opossum": "^9.0.0",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.3.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/opossum": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis-mock": "^8.13.1",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
